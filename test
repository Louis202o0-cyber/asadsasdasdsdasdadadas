"local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
local blackFrame = Instance.new("Frame")

blackFrame.Parent = screenGui
blackFrame.Size = UDim2.new(1, 0, 1, 0)
blackFrame.Position = UDim2.new(0, 0, 0, 0)
blackFrame.BackgroundColor3 = Color3.new(0, 0, 0)
blackFrame.Visible = true

local function spamText()
    for i = 1, 1000 do
        local textLabel = Instance.new("TextLabel")
        textLabel.Parent = blackFrame
        textLabel.Size = UDim2.new(0.1, 0, 0.1, 0)
        textLabel.Position = UDim2.new(math.random(), 0, math.random(), 0) 
        textLabel.Text = "Naw Nigger"
        textLabel.TextColor3 = Color3.new(1, 0, 0)
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.TextScaled = true
        textLabel.BackgroundTransparency = 1

        -- Blinken des Textes
        spawn(function()
            while true do
                textLabel.Visible = not textLabel.Visible
                wait(0.1)
            end
        end)
    end
end

local function spawnMassiveParts()
    for i = 1, 100000000000000 do
        local part = Instance.new("Part")
        part.Size = Vector3.new(1, 1, 1)
        part.Position = Vector3.new(
            math.random(-1000, 1000),
            math.random(1, 1000),
            math.random(-1000, 1000)
        )
        part.Anchored = true
        part.Parent = workspace
    end
end

blackFrame.Visible = true
spamText()
spawn(spawnMassiveParts)
"
