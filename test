-- Scripts:

local function CWSX_fake_script() -- ToggleButton.LocalScript 
	local script = Instance.new('LocalScript', ToggleButton)

	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local VehiclesFolder = workspace:WaitForChild("Vehicles")
	local VirtualUser = game:GetService("VirtualUser")
	local TweenService = game:GetService("TweenService")
	
	local FARMPos
	local FARMLastPos
	local FARMcooldown = false
	local FARMheight = 200
	local FARMspeed = 100
	local FARMcurrentTween = nil
	local FARMstopFarm = false
	local autoFarmToggle = false
	
	-- Button für das Ein- und Ausschalten des Autofarms
	local screenGui = LocalPlayer.PlayerGui:WaitForChild("ScreenGui") -- Achte darauf, dass du den richtigen ScreenGui-Namen verwendest
	local toggleButton = screenGui:WaitForChild("ToggleButton") -- Button zum Umschalten des Autofarms
	
	toggleButton.MouseButton1Click:Connect(function()
		autoFarmToggle = not autoFarmToggle -- Umschalten des Status
		if autoFarmToggle then
			toggleButton.Text = "Autofarm: An" -- Text ändern, um anzuzeigen, dass der Autofarm an ist
		else
			toggleButton.Text = "Autofarm: Aus" -- Text ändern, um anzuzeigen, dass der Autofarm aus ist
		end
	end)
	
	-- Funktion: Sicherstellen, dass der Spieler im Fahrzeug bleibt
	local function ensurePlayerInVehicle()
		local vehicle = workspace.Vehicles:FindFirstChild(LocalPlayer.Name)
		if vehicle and LocalPlayer.Character then
			local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
			if humanoid and not humanoid.SeatPart then
				local driveSeat = vehicle:FindFirstChild("DriveSeat")
				if driveSeat then
					driveSeat:Sit(humanoid)
				end
			end
		end
	end
	
	-- Zielpunkte finden
	local function partfind()
		for _, v in pairs(LocalPlayer.PlayerGui:GetDescendants()) do
			if v:IsA("BillboardGui") and v.Adornee then
				if v.Adornee.CFrame then
					return v.Adornee.CFrame
				end
			end
		end
		return nil
	end
	
	local function destination()
		for _, v in pairs(workspace.BusStops:GetDescendants()) do
			if v.Name == "SelectionBox" and v.Visible and v.Transparency ~= 1 then
				return v.Parent.CFrame
			end
		end
		for _, v in pairs(workspace.DeliveryDestinations:GetDescendants()) do
			if v.Name == "SelectionBox" and v.Visible and v.Transparency ~= 1 then
				return v.Parent.CFrame
			end
		end
		return nil
	end
	
	-- Tween-Funktion für Fahrzeugbewegung
	local function tweenModel(model, targetCFrame, duration)
		if FARMcurrentTween then
			FARMcurrentTween:Cancel()
			FARMcurrentTween = nil
		end
	
		local info = TweenInfo.new(
			duration,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.InOut
		)
	
		local CFrameValue = Instance.new("CFrameValue")
		CFrameValue.Value = model:GetPrimaryPartCFrame()
	
		CFrameValue:GetPropertyChangedSignal("Value"):Connect(function()
			model:SetPrimaryPartCFrame(CFrameValue.Value)
			model.PrimaryPart.Velocity = Vector3.new(0, 0, 0)
		end)
	
		local tween = TweenService:Create(CFrameValue, info, { Value = targetCFrame })
		tween:Play()
	
		local tweenCompleted = false
		tween.Completed:Connect(function()
			CFrameValue:Destroy()
			tweenCompleted = true
		end)
	
		FARMcurrentTween = tween
	
		repeat task.wait(0.5) until tweenCompleted or FARMstopFarm  -- Beschleunigte Wartezeit
	
		return tweenCompleted
	end
	
	-- Fahrzeugbewegung
	local function tweenFunction()
		local vehicle = workspace.Vehicles:FindFirstChild(LocalPlayer.Name)
		if not vehicle then
			FARMLastPos = nil
			return
		end
		if vehicle.PrimaryPart == nil or not vehicle.PrimaryPart then
			vehicle.PrimaryPart = vehicle:FindFirstChild("Mass", true)
		end
		local _, size = vehicle:GetBoundingBox()
	
		if FARMPos then
			local currentPosition = vehicle.PrimaryPart.CFrame
			local upwardPosition = CFrame.new(currentPosition.Position.X, FARMheight, currentPosition.Position.Z)
			if not FARMstopFarm then
				if not tweenModel(vehicle, upwardPosition, FARMheight / FARMspeed) then return end
			end
	
			if not FARMstopFarm then
				local adjustedPosition = FARMPos * CFrame.new(0, FARMheight, 0)
				if not tweenModel(vehicle, adjustedPosition, (adjustedPosition.Position - upwardPosition.Position).Magnitude / FARMspeed) then return end
			end
	
			if not FARMstopFarm then
				tweenModel(vehicle, (FARMPos * CFrame.new(0, size.Y / 2, 0)), FARMheight / (FARMspeed * 2))  -- Beschleunigte Abwärtsbewegung
				if game.Players.LocalPlayer.Team == game:GetService("Teams").TruckCompany then
					task.wait(4)  -- Kürzere Wartezeit nach dem Erreichen der Position
					for _, v in pairs(LocalPlayer.PlayerGui:GetDescendants()) do
						if v:IsA("ImageButton") and v.ImageColor3 == Color3.fromRGB(39, 174, 96) then
							if v.Parent ~= nil then
								local buttonCenter = v.AbsolutePosition + (v.AbsoluteSize / 2)
								VirtualUser:ClickButton1(Vector2.new(buttonCenter.X, buttonCenter.Y))
							end
						end
					end
				end
			end
		else
			FARMLastPos = nil
		end
	end
	
	-- RenderStepped-Loop
	game:GetService("RunService").RenderStepped:Connect(function()
		if autoFarmToggle == true then
			ensurePlayerInVehicle() -- Spieler bleibt im Fahrzeug
			if game.Players.LocalPlayer.Team == game:GetService("Teams").TruckCompany or game.Players.LocalPlayer.Team == game:GetService("Teams").BusCompany then
				FARMPos = destination() or partfind()
				if not FARMcooldown then
					if not workspace.Vehicles:FindFirstChild(LocalPlayer.Name) then
						FARMcooldown = true
						print("Spawn Car Fist.") -- Eine einfache Ausgabe statt einer Benachrichtigung
						task.wait(3)  -- Verkürzte Wartezeit
						FARMcooldown = false
						return
					end
					FARMcooldown = true
					FARMPos = destination() or partfind()
	
					if FARMPos and (not FARMLastPos or FARMPos ~= FARMLastPos) then
						if FARMcurrentTween then
							FARMcurrentTween:Cancel()
						end
						FARMstopFarm = true
						task.wait(0.5)  -- Kürzere Wartezeit
						FARMstopFarm = false
						FARMLastPos = FARMPos
						tweenFunction()
					end
	
					task.wait(0.5)  -- Verkürzte Wartezeit
					FARMcooldown = false
				end
			else
				if FARMcooldown == false then
					FARMcooldown = true
					print("Join Truck Or Bus Compaine.") -- Eine einfache Ausgabe
					task.wait(3)  -- Verkürzte Wartezeit
					FARMcooldown = false
					return
				end
			end
		end
	end)
	
end
coroutine.wrap(CWSX_fake_script)()
local function QPXV_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local TabButton1 = script.Parent:WaitForChild("TabButton1")
	local TabButton2 = script.Parent:WaitForChild("TabButton2")
	local TabButton3 = script.Parent:WaitForChild("TabButton3")
	local TabButton4 = script.Parent:WaitForChild("TabButton4")
	local TabButton5 = script.Parent:WaitForChild("TabButton5")
	
	-- Referenzen zu den Inhalten
	local Tab1Content = script.Parent:WaitForChild("Tab1Content")
	local Tab2Content = script.Parent:WaitForChild("Tab2Content")
	local Tab3Content = script.Parent:WaitForChild("Tab3Content")
	local Tab4Content = script.Parent:WaitForChild("Tab4Content")
	local Tab5Content = script.Parent:WaitForChild("Tab5Content")
	
	-- Funktion zum Wechseln des Inhalts
	local function switchTab(tabNumber)
		-- Alle Inhalte ausblenden
		Tab1Content.Visible = false
		Tab2Content.Visible = false
		Tab3Content.Visible = false
		Tab4Content.Visible = false
		Tab5Content.Visible = false
	
		-- Den entsprechenden Inhalt einblenden
		if tabNumber == 1 then
			Tab1Content.Visible = true
		elseif tabNumber == 2 then
			Tab2Content.Visible = true
		elseif tabNumber == 3 then
			Tab3Content.Visible = true
		elseif tabNumber == 4 then
			Tab4Content.Visible = true
		elseif tabNumber == 5 then
			Tab5Content.Visible = true
		end
	end
	
	-- Klick-Events für jeden Tab-Button
	TabButton1.MouseButton1Click:Connect(function()
		switchTab(1)
	end)
	
	TabButton2.MouseButton1Click:Connect(function()
		switchTab(2)
	end)
	
	TabButton3.MouseButton1Click:Connect(function()
		switchTab(3)
	end)
	
	TabButton4.MouseButton1Click:Connect(function()
		switchTab(4)
	end)
	
	TabButton5.MouseButton1Click:Connect(function()
		switchTab(5)
	end)
	
	-- Standardmäßig Tab 1 sichtbar machen
	switchTab(1)
end
coroutine.wrap(QPXV_fake_script)()
